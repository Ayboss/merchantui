{
  "name": "merchantsui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "node server.js",


    "test": "vitest",
    "preview": "vite preview",
    "postinstall": "husky install",
    "prettier-check": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,json}\"",
    "prettier-write": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,json}\"",
    "eslint": "eslint",
    "ts-check": "tsc-files-ignore-imports --noEmit --emitDeclarationOnly false",
    "lint": "sh -c 'eslint --fix $@ | snazzy'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.4.0",
    "jsdom": "^21.1.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "express": "^4.18.2",
    "react-auth-kit": "^2.12.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "standard": "^17.1.0",
    "styled-components": "^5.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.29.8",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5.11.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "snazzy": "^9.0.0",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0",
    "tsc-files-ignore-imports": "^1.1.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --concurrently 1"
    }
  },
  "lint-staged": {
    "*.{json,css,scss}": [
      "prettier"
    ],
    "*.{ts,tsx}": [
      "yarn ts-check"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
